generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id            String   @id
  username      String
  discriminator String
  avatar        String
  email         String
  emailVerified Boolean
  accessToken   String
  refreshToken  String
  createdAt     DateTime @default(now())
}
